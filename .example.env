# Supabase (from dashboard: Project URL, anon public key, service role key)
SUPABASE_URL=url
SUPABASE_ANON_KEY=key
SUPABASE_SECRET_KEY=key
SUPABASE_PROJECT_ID=id

# Required for client-side Supabase (use same URL and anon key as above)
NEXT_PUBLIC_SUPABASE_URL=url
NEXT_PUBLIC_SUPABASE_ANON_KEY=key

# App URL used in auth emails (magic link). Set to your production URL in prod so the link in the email is not localhost.
# In dev, omit this and the app uses the current origin.
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Resend (transactional email) â€” https://resend.com
RESEND_API_KEY=re_xxx

# Admin: only this email can access /admin (scheme CRUD, eligibility rules)
ADMIN_EMAIL=admin@example.com